<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<Pane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
      fx:controller="UserEditWindowController">
    <children>
        <GridPane layoutX="20.0" layoutY="20.0" prefHeight="330.0" prefWidth="561.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="272.0" minWidth="10.0" prefWidth="88.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="499.0" minWidth="10.0" prefWidth="473.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="62.0" minHeight="4.0" prefHeight="54.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="128.0" minHeight="10.0" prefHeight="58.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="163.0" minHeight="10.0" prefHeight="68.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="181.0" minHeight="10.0" prefHeight="74.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="189.0" minHeight="10.0" prefHeight="76.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label prefHeight="17.0" prefWidth="263.0" text="first_name" textAlignment="CENTER"/>
                <Label prefHeight="17.0" prefWidth="263.0" text="last_name" textAlignment="CENTER"
                       GridPane.rowIndex="1"/>
                <Label prefHeight="17.0" prefWidth="263.0" text="username" textAlignment="CENTER"
                       GridPane.rowIndex="2"/>
                <Label prefHeight="17.0" prefWidth="263.0" text="password" textAlignment="CENTER"
                       GridPane.rowIndex="3"/>
                <Label prefHeight="17.0" prefWidth="263.0" text="role" textAlignment="CENTER" GridPane.rowIndex="4"/>
                <TextField fx:id="firstNameEditText" GridPane.columnIndex="1"/>
                <TextField fx:id="lastNameEditText" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <TextField fx:id="userNameEditText" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <TextField fx:id="passwordEditText" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <TextField fx:id="roleEditText" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
            </children>
        </GridPane>
        <Button fx:id="okEditButton" layoutX="476.0" layoutY="361.0" mnemonicParsing="false" onAction="#handleOk"
                text="ok"/>
        <Button fx:id="cancelEditButton" layoutX="519.0" layoutY="361.0" mnemonicParsing="false"
                onAction="#handleCancel" text="cancel"/>
    </children>
</Pane>
