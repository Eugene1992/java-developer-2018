<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="EmployeeEditDialogController">
    <children>
        <GridPane layoutX="14.0" layoutY="14.0" prefHeight="180.0" prefWidth="467.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="290.0" minWidth="10.0" prefWidth="67.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="349.0" minWidth="0.0" prefWidth="349.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="First name"/>
                <Label text="Last name" GridPane.rowIndex="1"/>
                <Label text="Age" GridPane.rowIndex="2"/>
                <Label text="Salary" GridPane.rowIndex="3"/>
                <Label text="Is married" GridPane.rowIndex="4"/>
                <Label text="Position" GridPane.rowIndex="5"/>
                <TextField fx:id="firstNameField" GridPane.columnIndex="1"/>
                <TextField fx:id="lastNameField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <TextField fx:id="ageField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <TextField fx:id="salaryField" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <TextField fx:id="positionField" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
                <CheckBox fx:id="isMarriedCheckBox" mnemonicParsing="false" GridPane.columnIndex="1"
                          GridPane.rowIndex="4"/>
            </children>
        </GridPane>
        <Button layoutX="434.0" layoutY="261.0" mnemonicParsing="false" onAction="#handleCancel" text="Cancel"/>
        <Button layoutX="370.0" layoutY="261.0" mnemonicParsing="false" onAction="#handleOk" prefHeight="25.0"
                prefWidth="52.0" text="OK"/>
    </children>
</AnchorPane>
